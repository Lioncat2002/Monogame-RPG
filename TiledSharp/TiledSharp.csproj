<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net35;net40;net45</TargetFrameworks>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <PackageId>TiledSharp</PackageId>
    <Version>1.0.0</Version>
    <Authors>Marshall Ward</Authors>
    <PackageDescription>C# library for parsing and importing TMX and TSX files generated by Tiled, a tile map generation tool.</PackageDescription>
    <Copyright>Copyright Marshall Ward 2018</Copyright>
    <PackageLicenseUrl>https://github.com/marshallward/TiledSharp/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/marshallward/TiledSharp</PackageProjectUrl>
    <RepositoryUrl>https://github.com/marshallward/TiledSharp</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
  </PropertyGroup>

</Project>